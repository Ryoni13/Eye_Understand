<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="air.korea.mybatis.TimeAirMapper">

    
    
	<select id="getTimeList" resultType="TimeAirVO">
		select * from airtime order by start_place
	</select>

	<select id="timeSearch" parameterType="TimeAirVO" resultType="TimeAirVO">
		select * from airtime where start_place=#{start_place} and end_place=#{end_place}
	</select>
</mapper>