<mapper namespace="EyeUnderstand.mybatis.EyeUnderstand">
    
    <select id = "memberLogin"  parameterType="MemberVO" resultType="MemberVO">
        
		select * from eyemember where ID=#{id, jdbcType=VARCHAR} and PW=#{pw, jdbcType=VARCHAR}
    </select>

    <select id="getTestList" parameterType ="java.util.Map" resultType="EyeUnderstand.model.testVO">
      select * from work where work like ('${serchInput}%')
   </select>
    
    
